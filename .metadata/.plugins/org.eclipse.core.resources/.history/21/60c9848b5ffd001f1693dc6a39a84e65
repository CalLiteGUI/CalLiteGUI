package com.limno.calgui;

import javax.swing.JCheckBox;
import javax.swing.JInternalFrame;

import java.awt.event.*;
import java.awt.*;

public class MyCheckBox extends JCheckBox {
	String switchIDstr;
	String FNstr;
	
	void setSID (String switchID) { switchIDstr = switchID;}
    String returnSID () {return switchIDstr;}
	void setFNID (String filename) { FNstr = filename;}
    String returnFNID () {return FNstr;}
	}
