<?xml version="1.0" encoding="UTF-8"?>
<panel id="RunSettings" constraints="BorderLayout.TOP" layout="GridBagLayout(rowWeights(0.1, 1, 0, 0, 0, 0, 0, 0))">
	<label text="Scenario">
		<gridbagconstraints insets="0,0,5,5" gridx="0" gridy="0" gridwidth="1" anchor="GridBagConstraints.NORTHWEST" fill="GridBagConstraints.BOTH"/>
	</label>
	<textfield text="">
		<gridbagconstraints insets="0,0,5,5" gridx="1" gridy="0" weightx="1" gridwidth="5" anchor="GridBagConstraints.NORTHWEST" fill="GridBagConstraints.HORIZONTAL"/>
	</textfield>
	<label text="Scenario Description">
		<gridbagconstraints insets="0,0,5,5" gridx="0" gridy="1" gridwidth="2" anchor="GridBagConstraints.NORTHWEST"/>
	</label>
	<textpane text="">
		<gridbagconstraints insets="0,0,5,5" gridx="1" gridy="2" weightx="1" gridwidth="5" anchor="GridBagConstraints.NORTHWEST" fill="GridBagConstraints.HORIZONTAL"/>
	</textpane>
	<label text="Run Period">
		<gridbagconstraints insets="100,0,5,5" gridx="0" gridy="3" gridwidth="2" anchor="GridBagConstraints.NORTHWEST"/>
	</label>
	<label text="Directory for run:">
		<gridbagconstraints insets="100,0,5,5" gridx="3" gridy="3" gridwidth="1" anchor="GridBagConstraints.NORTHWEST"/>
	</label>
	
	<textfield text="test" id="tbRSdir" enabled="false">
		<gridbagconstraints insets="0,0,5,5" gridx="3" gridy="4" weightx="1" gridwidth="5" anchor="GridBagConstraints.NORTHWEST" fill="GridBagConstraints.HORIZONTAL"/>
	</textfield>
	
	<label text="Probabilistic/deterministic settings">
		<gridbagconstraints insets="100,0,5,5" gridx="0" gridy="5" gridwidth="2" anchor="GridBagConstraints.NORTHWEST"/>
	</label>
	<label text="Batch Runs">
		<gridbagconstraints insets="100,0,5,5" gridx="3" gridy="5" gridwidth="1" anchor="GridBagConstraints.NORTHWEST"/>
	</label>
</panel>


